<?xml version="1.0" encoding="UTF-8"?>
<tc:tc-config xsi:schemaLocation="http://www.terracotta.org/config http://www.terracotta.org/schema/terracotta-4.xsd"
xmlns:tc="http://www.terracotta.org/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers> 
   <server host="%i" name="FW_TC_SERVER">
	<data>logs/terracotta/server/data</data>
	<logs>logs/terracotta/server/logs</logs>
	<statistics>logs/terracotta/server/statistics</statistics>
	<dso-port>8510</dso-port>
	<jmx-port>8520</jmx-port>
  </server>
</servers>
<clients>
	<logs>logs/terracotta/client/logs/%D</logs>
</clients>
</tc:tc-config>
